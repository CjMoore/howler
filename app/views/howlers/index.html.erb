<div class = 'container'>
  <h1 class='center'>My Howlers</h1>

  <div class="row">
  <% @howlers.each do |howler| %>
      <div class="col s4">
        <div class="card blue-grey darken-1">
          <div class="card-content white-text">
            <div class="data" data-openness="<%= howler.openness %>" data-conscientiousness="<%= howler.conscientiousness %>" data-extraversion="<%= howler.extraversion %>" data-agreeableness="<%= howler.agreeableness %>" data-emotional-range="<%= howler.emotional_range %>">
            <span class="card-title"><%= link_to howler.title %></span>
              <script id="small-graph"></script>
              <div id="small-graph">

              <%= javascript_include_tag("/app/assets/javascripts/individual_graphs.js") %>
                <!-- $(".small-graph").add(doSocialChart) -->
              </div>
          </div>
          </div>
        </div>
      </div>
  <% end %>
  </div>
</div>
